<h1>New textquestion</h1>

<% form_for(@textquestion) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :question_text %><br />
    <%= f.text_area :question_text %>
  </p>
  <p>
    <%= f.label :question_comment %><br />
    <%= f.text_area :question_comment %>
  </p>
  <p>
    <%= f.label :question_type %><br />
    <%= f.text_field :question_type %>
  </p>
  <p>
    <%= f.label :survey %><br />
    <%= f.text_field :survey %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', textquestions_path %>