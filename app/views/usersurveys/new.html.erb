<h1>New usersurvey</h1>

<% form_for(@usersurvey) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :taken %><br />
    <%= f.check_box :taken %>
  </p>
  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.label :survey %><br />
    <%= f.text_field :survey %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', usersurveys_path %>