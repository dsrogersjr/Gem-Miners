<h1>Editing threesixty</h1>

<% form_for(@threesixty) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :threesixty_name %><br />
    <%= f.text_field :threesixty_name %>
  </p>
  <p>
    <%= f.label :threesixty_intro %><br />
    <%= f.text_area :threesixty_intro %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @threesixty %> |
<%= link_to 'Back', threesixties_path %>